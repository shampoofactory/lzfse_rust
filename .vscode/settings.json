{
    "rust-analyzer.checkOnSave.command": "clippy",
    "rust-analyzer.inlayHints.enable": false,
    "cSpell.ignoreWords": [
        "accum",
        "cflags",
        "dezeroed",
        "encodable",
        "hasher",
        "libc",
        "llvm",
        "lmd",
        "lmds",
        "lzfoo",
        "lzfse",
        "lzvn",
        "memset",
        "nbits",
        "nclz",
        "nctz",
        "noalias",
        "nonoverlapping",
        "nonpack",
        "nostack",
        "proto",
        "rustc",
        "shampoofactory",
        "subcommand",
        "subslices",
        "udef",
        "uninit",
        "useq",
    ],
    "files.watcherExclude": {
        "**/target/**": true
    }
}